{
  "name": "mobile-app-automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:headed",
    "test:headed": "npx wdio run wdio.conf.js",
    "test:headless": "HEADLESS=true npx wdio run wdio.conf.js",
    "start:appium": "appium",
    "list:devices": "adb devices",
    "device:info": "adb shell getprop ro.build.version.release && adb shell getprop ro.product.model",
    "install:app": "adb install path/to/your/app.apk",
    "uninstall:app": "adb uninstall com.your.app.package",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@wdio/cli": "^9.19.2",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "webdriverio": "^9.19.2"
  },
  "devDependencies": {
    "@wdio/appium-service": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "appium": "^3.0.2",
    "appium-uiautomator2-driver": "^5.0.1"
  }
}
